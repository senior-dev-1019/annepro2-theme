{%- if rootTemplate == '404' -%}
  <script>
    window.PSEO_LICENSED_TO = '{{- licensedTo-}}';
    window.PSEO_APP_PROXY_PATH = '{{- appProxyPath -}}';
    (function() {
      var loadScript = function(url) {
        var script = document.createElement("script");
        script.type = "text/javascript";

        script.src = url;
        document.getElementsByTagName("head")[0].appendChild(script);
      }

      loadScript('{{- appProxyPath | append: '/pluginseo_404.js' -}}');
    })()
  </script>
{%- endif -%}